.h1 {
  font-weight: bold;
  text-align: center;
  font-size: 18px;
  padding: 50rpx;
  margin-bottom: 100rpx;
  margin-top: 50rpx;
}

.dates {
  display: flex;
  justify-content: space-between;
  margin-left: 100rpx;
  margin-right: 100rpx;
  font-size: 14px;
}

.dates .title {
  font-weight: bold;
  margin-bottom: 25rpx;
}

.button-dark {
  background-color: black;
  width: 540rpx;
  border-radius: 10rpx;
  margin: 0 auto;
}

.button-grey {
  background-color: white;
  width: 540rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  border: 1px solid rgb(163, 163, 163);
  margin-top: 20rpx;

}

.btn-black {
  width: 200rpx;
  color: white;
  background-color: black;
  padding: 20rpx;
  padding-top: 25rpx;
  border-radius: 10rpx;
  font-weight: bold;
  font-size: 12px;
  margin-top: 90rpx;
}

.btn-grey {
  width: 200rpx;
  color: rgb(163, 163, 163);
  background-color: white;
  padding: 20rpx;
  padding-top: 25rpx;
  border-radius: 10rpx;
  font-size: 12px;
  font-weight: light;
} 